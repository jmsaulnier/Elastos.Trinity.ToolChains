{
  "name": "@elastosfoundation/trinity-cli",
  "version": "1.0.4",
  "description": "Command line tool to manage Elastos Trinity DApps",
  "main": "./bin/trinity",
  "bin": "./bin/trinity",
  "scripts": {
    "build": "cross-env BABEL_ENV=production babel src --out-dir bin",
    "rename": "mv bin/trinity.js bin/trinity",
    "copytoolchain": "mkdir -p bin/toolchain && cp ../bin/* bin/toolchain",
    "copyassets": "mkdir -p bin/assets && cp -rf src/assets bin/",
    "getepksigner": "./scripts/getepksigner.sh",
    "prepublish": "npm run devprepare && npm run getepksigner",
    "devprepare": "npm run copytoolchain && npm run copyassets && npm run build && npm run rename"
  },
  "files": [
    "bin",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elastos/Elastos.Trinity.ToolChains.git"
  },
  "keywords": [
    "trinity",
    "elastos",
    "tool",
    "command line"
  ],
  "author": "Elastos Foundation",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elastos/Elastos.Trinity.ToolChains/issues"
  },
  "homepage": "https://www.elastos.org",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.1.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "mocha": "^6.1.3",
    "nyc": "^13.3.0",
    "rimraf": "^2.6.2"
  },
  "directories": {
    "bin": "bin"
  },
  "dependencies": {
    "edit-json-file": "^1.3.1",
    "email-validator": "^2.0.4",
    "fs-extra": "^8.1.0",
    "prompts": "^2.2.1",
    "yargs": "^13.3.0"
  }
}
